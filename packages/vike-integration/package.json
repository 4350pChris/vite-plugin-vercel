{
  "name": "@vite-plugin-vercel/vike",
  "version": "4.0.1",
  "type": "module",
  "files": [
    "dist",
    "templates",
    "*.d.ts",
    "helpers.js",
    "config.js"
  ],
  "description": "Use vike with vite-plugin-vercel",
  "author": "JoÃ«l Charles <joel.charles91@gmail.com>",
  "repository": "https://github.com/magne4000/vite-plugin-vercel",
  "main": "./dist/vike.js",
  "types": "./dist/vike.d.ts",
  "exports": {
    ".": "./dist/vike.js",
    "./helpers": "./dist/templates/helpers.js",
    "./config": {
      "types": "./config.d.ts",
      "default": "./dist/+config.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepack": "rm -rf dist && pnpm build",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint:ts": "eslint . --max-warnings 0 --ignore-pattern dist"
  },
  "devDependencies": {
    "@types/node": "^16.18.76",
    "@vercel/node": "^3.0.17",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vike": "^0.4.160",
    "vite": "^5.0.12",
    "vite-plugin-vercel": "workspace:*"
  },
  "dependencies": {
    "@brillout/libassert": "^0.5.8",
    "nanoid": "^5.0.4"
  },
  "peerDependencies": {
    "vike": "*",
    "vite": "^4.4 || ^5.0.2",
    "vite-plugin-vercel": "*"
  },
  "license": "MIT"
}
