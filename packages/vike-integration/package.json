  "main": "./dist/vite-plugin-ssr.js",
  "types": "./dist/vite-plugin-ssr.d.ts",
  "exports": {
    ".": "./dist/vite-plugin-ssr.js",
    "./helpers": "./dist/templates/helpers.js",
