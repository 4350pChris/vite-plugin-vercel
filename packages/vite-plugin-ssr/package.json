{
  "name": "@magne4000/vite-plugin-vercel-ssr",
  "version": "0.1.4",
  "type": "module",
  "files": [
    "dist",
    "templates",
    "*.d.ts",
    "helpers.js"
  ],
  "description": "Use vite-plugin-ssr with vite-plugin-vercel",
  "author": "JoÃ«l Charles <joel.charles91@gmail.com>",
  "repository": "https://github.com/magne4000/vite-plugin-vercel",
  "main": "./dist/vite-plugin-ssr.cjs",
  "module": "./dist/vite-plugin-ssr.js",
  "exports": {
    ".": {
      "types": "./dist/vite-plugin-ssr.d.ts",
      "import": "./dist/vite-plugin-ssr.js",
      "require": "./dist/vite-plugin-ssr.cjs"
    },
    "./helpers": {
      "types": "./dist/templates/helpers.d.ts",
      "import": "./dist/templates/helpers.js",
      "require": "./dist/templates/helpers.cjs"
    }
  },
  "types": "./dist/vite-plugin-ssr.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepack": "rm -rf dist && pnpm build",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint:ts": "eslint . --max-warnings 0 --ignore-pattern dist"
  },
  "devDependencies": {
    "@brillout/libassert": "^0.5.8",
    "@types/node": "^16.18.21",
    "@vercel/node": "^2.10.0",
    "nanoid": "^4.0.2",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vite-plugin-ssr": "^0.4.103",
    "vite-plugin-vercel": "workspace:*"
  },
  "license": "MIT"
}
