{
  "name": "vite-plugin-vercel",
  "version": "0.3.0",
  "type": "module",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./index.d.ts",
  "description": "Vercel adapter for vite",
  "author": "JoÃ«l Charles <joel.charles91@gmail.com>",
  "repository": "https://github.com/magne4000/vite-plugin-vercel",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepack": "rm -rf dist && pnpm build",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint:ts": "eslint . --max-warnings 0 --ignore-pattern dist"
  },
  "peerDependencies": {
    "vite": "^4.2.0",
    "@vite-plugin-vercel/vike": "workspace:*",
    "vite-plugin-ssr": "*"
  },
  "peerDependenciesMeta": {
    "@vite-plugin-vercel/vike": {
      "optional": true
    },
    "vite-plugin-ssr": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^16.18.48",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.48.0",
    "tsup": "^6.7.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "dependencies": {
    "@brillout/libassert": "^0.5.8",
    "@vercel/routing-utils": "^2.2.1",
    "esbuild": "^0.17.19",
    "fast-glob": "^3.3.1",
    "zod": "^3.22.2"
  }
}
