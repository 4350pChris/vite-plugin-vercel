    "@vercel/routing-utils": "^3.1.0",
    "esbuild": "^0.19.11",
    "eval": "^0.1.8",
    "fast-glob": "^3.3.2",
    "magicast": "^0.3.3",
    "zod": "^3.22.4"
  }
