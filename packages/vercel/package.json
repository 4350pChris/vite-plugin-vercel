    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vike": "^0.4.157",
    "vite": "^5.0.11"
  },
  "dependencies": {
    "@brillout/libassert": "^0.5.8",
