    "*.d.ts"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./index.d.ts",
  "description": "Vercel adapter for vite",
  "author": "JoÃ«l Charles <joel.charles91@gmail.com>",
