{
  "name": "vite-plugin-vercel-workspace-root",
  "private": "true",
  "scripts": {
    "dev:all": "pnpm -r --parallel dev --filter ./packages",
    "dev": "ts-node ./server/index.ts",
    "build:all": "pnpm -r build --filter ./packages",
    "build": "vite build && vite build --ssr",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "test:1": "ts-node-esm tests/01-minimal/before.ts && jest tests/01-minimal",
    "test:2": "ts-node-esm tests/02-additional-endpoints/before.ts && jest tests/02-additional-endpoints",
    "test:3": "ts-node-esm tests/03-ssr/before.ts && jest tests/03-ssr",
    "test:4": "ts-node-esm tests/04-isr/before.ts && jest tests/04-isr",
    "test:5": "ts-node-esm tests/05-vite-plugin-ssr/before.ts && jest tests/05-vite-plugin-ssr",
    "pretest": "pnpm run build:all",
    "test": "pnpm run test:1 && pnpm run test:2 && pnpm run test:3 && pnpm run test:4 && pnpm run test:5"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@vercel/build-utils": "^2.14.0",
    "@vercel/node": "^1.13.0",
    "fast-glob": "^3.2.11",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "myzod": "^1.8.7",
    "prettier": "^2.5.1",
    "rollup": "^2.70.0",
    "ts-jest": "^27.1.3",
    "vitest": "^0.6.0"
  },
  "dependencies": {
    "@brillout/libassert": "^0.5.4",
    "@vitejs/plugin-react": "^1.2.0",
    "cross-env": "^7.0.3",
    "express": "^4.17.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vite-plugin-ssr": "^0.3.59",
    "vite-plugin-vercel": "^1.0.0"
  }
}
