{
  "name": "vite-plugin-vercel-workspace-root",
  "private": "true",
  "scripts": {
    "dev:all": "pnpm -r --parallel dev --filter ./packages",
    "dev": "ts-node ./server/index.ts",
    "build:all": "pnpm -r build --filter ./packages",
    "build": "vite build && vite build --ssr",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "test:01": "jest tests/01-minimal",
    "test:02": "jest tests/02-additional-endpoints",
    "test:03": "jest tests/03-ssr",
    "test:04": "jest tests/04-isr",
    "test:05": "jest tests/05-vite-plugin-ssr",
    "pretest": "pnpm run build:all",
    "test": "jest",
    "prepublishOnly": "pnpm run build:all && jest",
    "publish:beta": "pnpm run prepublishOnly && pnpm publish -r --tag beta",
    "publish:stable": "pnpm run prepublishOnly && pnpm publish -r"
  },
  "devDependencies": {
    "@swc/core": "^1.2.186",
    "@swc/jest": "^0.2.21",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.34",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@vercel/build-utils": "^3.0.1",
    "@vercel/node": "^1.15.2",
    "fast-glob": "^3.2.11",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.1",
    "nanoid": "^3.3.4",
    "prettier": "^2.6.2",
    "zod": "^3.16.0"
  },
  "dependencies": {
    "@brillout/libassert": "^0.5.5",
    "@vitejs/plugin-react": "^1.3.2",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-ssr": "^0.3.64",
    "vite-plugin-vercel": "^0.0.4"
  }
}
