{
  "name": "vite-plugin-vercel-workspace-root",
  "private": "true",
  "type": "module",
  "scripts": {
    "dev:all": "pnpm -r --filter \"./packages/*\" --parallel dev",
    "dev": "esno ./server/index.ts",
    "build:all": "pnpm -r --filter \"./packages/*\" build",
    "build": "vite build && vite build --ssr",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "test:01": "jest tests/01-minimal",
    "test:02": "jest tests/02-additional-endpoints",
    "test:03": "jest tests/03-prerender",
    "test:04": "jest tests/04-isr",
    "test:05": "jest tests/05-vite-plugin-ssr",
    "test": "pnpm run build:all && jest",
    "prepublishOnly": "pnpm run build:all && jest && pnpm changeset version",
    "publish:beta": "pnpm run prepublishOnly && pnpm changeset publish -r --tag beta",
    "publish:stable": "pnpm run prepublishOnly && pnpm changeset publish -r"
  },
  "devDependencies": {
    "@brillout/libassert": "^0.5.6",
    "@changesets/cli": "^2.22.0",
    "@types/node": "^17.0.36",
    "cross-env": "^7.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.3",
    "nanoid": "^3.3.4",
    "prettier": "^2.6.2",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vite-plugin-ssr": "^0.3.64",
    "vite-plugin-vercel": "workspace:^"
  }
}
