    "@vercel/og": "^0.5.17",
    "@vite-plugin-vercel/vike": "workspace:*",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "cross-fetch": "^4.0.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
