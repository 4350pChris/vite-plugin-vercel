    "vitest": "^0.34.3",
    "zod": "^3.22.2"
  },
  "dependencies": {
    "@brillout/libassert": "0.5.6",
    "@vercel/edge-config": "^0.3.0",
    "@vite-plugin-vercel/vike": "workspace:*",
