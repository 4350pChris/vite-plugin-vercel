{
	"compilerOptions": {
		"strict": true,
		"lib": ["ES2020", "DOM"],
		"resolveJsonModule": true,
		"module": "ES2020",
		"esModuleInterop": true,
		"isolatedModules": true,
		"jsx": "react",
		"target": "es6",
		"moduleResolution": "node",
		"types": ["node"],
		"skipLibCheck": true
	},
	"include": ["src/**/*.ts"]
}
